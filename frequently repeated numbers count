#include <stdio.h>

#define SIZE 100

int main() {
    int arr[] = {4, 5, 6, 4, 6, 7, 4, 5, 6, 8, 9, 6};
    int n = sizeof(arr) / sizeof(arr[0]);

    int count[SIZE] = {0};
    int visited[SIZE] = {0};

    // Count frequency
    for (int i = 0; i < n; i++) {
        int alreadyVisited = 0;

        for (int k = 0; k < i; k++) {
            if (arr[i] == arr[k]) {
                alreadyVisited = 1;
                break;
            }
        }

        if (alreadyVisited == 0) {
            int freq = 1;
            for (int j = i + 1; j < n; j++) {
                if (arr[i] == arr[j]) {
                    freq++;
                }
            }

            if (freq > 1) {
                printf("Number %d is repeated %d times\n", arr[i], freq);
            }
        }
    }

    return 0;
}
